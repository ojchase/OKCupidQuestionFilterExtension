{

  "description": "Allows filtering of answered questions on OKCupid.com dating profiles according to user-defined categories",
  "manifest_version": 2,
  "name": "OKCupidQuestionFilter",
  "version": "1.0",
  "homepage_url": "https://github.com/ojchase/OKCupidQuestionFilterExtension",
  "icons": {
    "48": "icons/archer.svg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/archer.svg",
    "default_title": "OKCupidQuestionFilter",
    "default_popup": "popup/choose_beast.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*.okcupid.com/*", "*://okcupid.com/*"],
      "js": ["lib/jquery-3.5.1.min.js", "content_scripts/filter.js", "content_scripts/beastify.js"]
    }
  ],

  "web_accessible_resources": [
    "question_data.csv"
  ]

}
